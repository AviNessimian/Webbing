{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "NetworkEvent": {
    "Consumers": {
      "PartitionIndex": 0,
      "Partitions": 1,
      "PeriodInMilliseconds": 1000,
      "ThresholdTimeInHours": 24,
      "ProcessLimit": 200
    }
  }
  
}
